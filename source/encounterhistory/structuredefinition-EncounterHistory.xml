  <version value="5.0.0"/>
  <name value="EncounterHistory"/>
  <status value="draft"/>
  <date value="2021-01-02T08:58:31+11:00"/>
  <publisher value="Health Level Seven International (Patient Administration)"/>
  <contact>
    <telecom>
      <system value="url"/>
