<StructureDefinition xmlns='http://hl7.org/fhir'>
  <id value='document-bundle'/>
  <extension url='http://hl7.org/fhir/StructureDefinition/structuredefinition-category'>
    <valueString value='Foundation.Other'/>
  </extension>
  <extension url='http://hl7.org/fhir/StructureDefinition/structuredefinition-security-category'>
    <valueCode value='not-classified'/>
  </extension>
  <url value='http://hl7.org/fhir/StructureDefinition/document-bundle'/>
  <version value='0.1.0'/>
  <name value='DocumentBundle'/>
  <title value='Document bundle'/>
  <status value='active'/>
  <description value='This profile holds all the requirements and constraints related to a FHIR document.'/>
  <fhirVersion value='5.0.0'/>
  <mapping>
    <identity value='v2'/>
    <uri value='http://hl7.org/v2'/>
    <name value='HL7 v2 Mapping'/>
  </mapping>
  <mapping>
    <identity value='rim'/>
    <uri value='http://hl7.org/v3'/>
    <name value='RIM Mapping'/>
  </mapping>
  <mapping>
    <identity value='cda'/>
    <uri value='http://hl7.org/v3/cda'/>
    <name value='CDA (R2)'/>
  </mapping>
  <mapping>
    <identity value='w5'/>
    <uri value='http://hl7.org/fhir/fivews'/>
    <name value='FiveWs Pattern Mapping'/>
  </mapping>
  <kind value='resource'/>
  <abstract value='false'/>
  <type value='Bundle'/>
  <baseDefinition value='http://hl7.org/fhir/StructureDefinition/Bundle'/>
  <derivation value='constraint'/>
  <differential>
    <element id='Bundle.identifier'>
      <path value='Bundle.identifier'/>
      <min value='1'/>
    </element>
    <element id='Bundle.identifier.system'>
      <path value='Bundle.identifier.system'/>
      <min value='1'/>
    </element>
    <element id='Bundle.identifier.value'>
      <path value='Bundle.identifier.value'/>
      <min value='1'/>
    </element>
    <element id='Bundle.type'>
      <patternCode value='document'/>
      <path value='Bundle.type'/>
    </element>
    <element id='Bundle.timestamp'>
      <path value='Bundle.timestamp'/>
      <min value='1'/>
    </element>
    <element id='Bundle.total'>
      <path value='Bundle.total'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry'>
      <slicing>
        <discriminator>
          <type value='exists'/>
          <path value='entry'/>
        </discriminator>
        <description value='Slice to enforce first entry is Composition.'/>
        <ordered value='true'/>
        <rules value='openAtEnd'/>
      </slicing>
      <path value='Bundle.entry'/>
      <min value='1'/>
    </element>
    <element id='Bundle.entry:first'>
      <path value='Bundle.entry'/>
      <sliceName value='first'/>
      <min value='1'/>
      <max value='1'/>
    </element>
    <element id='Bundle.entry:first.fullUrl'>
      <path value='Bundle.entry.fullUrl'/>
      <min value='1'/>
    </element>
    <element id='Bundle.entry:first.resource'>
      <type>
        <code value='Composition'/>
      </type>
      <path value='Bundle.entry.resource'/>
      <min value='1'/>
    </element>
    <element id='Bundle.entry:first.search'>
      <path value='Bundle.entry.search'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry:first.request'>
      <path value='Bundle.entry.request'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry:first.response'>
      <path value='Bundle.entry.response'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry:other'>
      <path value='Bundle.entry'/>
      <sliceName value='other'/>
      <min value='0'/>
      <max value='*'/>
    </element>
    <element id='Bundle.entry:other.fullUrl'>
      <path value='Bundle.entry.fullUrl'/>
      <min value='1'/>
    </element>
    <element id='Bundle.entry:other.resource'>
      <path value='Bundle.entry.resource'/>
      <min value='1'/>
    </element>
    <element id='Bundle.entry:other.search'>
      <path value='Bundle.entry.search'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry:other.request'>
      <path value='Bundle.entry.request'/>
      <max value='0'/>
    </element>
    <element id='Bundle.entry:other.response'>
      <path value='Bundle.entry.response'/>
      <max value='0'/>
    </element>
    <element id='Bundle.issues'>
      <path value='Bundle.issues'/>
      <max value='0'/>
    </element>
  </differential>
</StructureDefinition>